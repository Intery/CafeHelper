msgid "Edit a Timer"
msgstr "Bir sayacı düzenle"

#: src/modules/pomodoro/cog.py:690
msgctxt "cmd:pomodoro_edit|param:channel"
msgid "timer_channel"
msgstr "sayaç_kanalı"

#: src/modules/pomodoro/cog.py:696
msgctxt "cmd:pomodoro_edit|param:channel|desc"
